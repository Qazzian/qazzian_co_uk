<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title>{{#page.title}}{{page.title}} - {{/page.title}}{{config.siteName}}</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="description" content="">
		<meta name="keywords" content="">
		<meta name="author" content="{{config.contact}}">

		<script type="text/javascript">
			if (location.hostname.match(/revertity.com/)) {
				location.reload();
			}
		</script>

		<!-- styles -->
		{{>css}}

		<!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
		<!--[if lt IE 9]>
			<script src="{{config.assetRoot}}/js/html5shiv.js"></script>
		<![endif]-->

		<!-- fav and touch icons -->
		<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{config.assetRoot}}/ico/apple-touch-icon-144-precomposed.png">
		<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{config.assetRoot}}/ico/apple-touch-icon-114-precomposed.png">
		<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{config.assetRoot}}/ico/apple-touch-icon-72-precomposed.png">
		<link rel="apple-touch-icon-precomposed" href="{{config.assetRoot}}/ico/apple-touch-icon-57-precomposed.png">
		<link rel="shortcut icon" href="{{config.assetRoot}}/ico/favicon.png">
	</head>

	<body data-spy="scroll" data-target=".bs-docs-sidebar">

		<!-- Navbar ================================================== -->
		<header class="navbar navbar-inverse">
			<div class="navbar-inner">
				<div class="container">
					<a class="brand" href="{{pages.0.url}}">{{config.siteName}}</a>
					{{>MainMenu}}
				</div>
			</div>
		</header>

		<main class="container">
			{{>page}}
		</main>

		

		{{>footer}}
		{{>js}}

	</body>
</html>
